{
  "expo": {
    "name": "Archi Select",
    "slug": "archi-select",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Logo-ArchiSelect.png",
    "scheme": "archi-select",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.archiselect.app"
    },
    "android": {
      "package": "com.archiselect.app",
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/Logo-ArchiSelect.png",
        "backgroundImage": "./assets/images/Logo-ArchiSelect.png",
        "monochromeImage": "./assets/images/Logo-ArchiSelect.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/Logo-ArchiSelect.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Logo-ArchiSelect.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b257401b-754b-4fc5-a76f-885ba5b05e96"
      }
    }
  }
}
